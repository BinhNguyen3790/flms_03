<% provide(:title, t(".home")) %>
<div class="container">
  <div class="center jumbotron">
    <h1><%= t(".heading1") %></h1>
    <h2>
      <%= t(".heading2") %>
      <a href="https://www.railstutorial.org/"><%= t(".a") %></a>
      <%= t(".last_heading2") %>
    </h2>
    <%= link_to t(".signup"), signup_path, class: "btn btn-lg btn-primary" %>
  </div>
  <h3><%= t(".book") %></h3>
  <ul class="books">
    <%= render @books %>
    <%= will_paginate @books %>
  </ul>
</div>
